Bassman_root_compute :: (vars: *Bassman_R_Vars, a: *float32, b: *float32)
{
    for c: 0..num_ports-1 {
        b[c] = vars.S[c] * a[0];
        for r: 1..num_ports-1 {
            b[c] += vars.S[r * num_ports + c] * a[r];
        }
    }
}
